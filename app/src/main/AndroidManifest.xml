<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wcare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".hygieneDetailed"></activity>
        <activity android:name=".mentalHealthActivity" />
        <activity android:name=".pregnancy" />
        <activity android:name=".beforeActivity" />
        <activity android:name=".AfterActivity" />
        <activity android:name=".hygiene" />
        <activity android:name=".fragments.CommunityFragment" />
        <activity android:name=".NavigationPage" />
        <activity android:name=".SignIn" />
        <activity android:name=".RegistrationPage" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>